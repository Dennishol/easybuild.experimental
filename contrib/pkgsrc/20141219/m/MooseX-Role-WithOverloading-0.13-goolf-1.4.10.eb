easyblock = 'ConfigureMake'

name = 'p5-'
version = 'Role-WithOverloading-0.13'
versionsuffix = '-20141219'

description = "MooseX-Role-WithOverloading-0.13: Moose extension for roles which support overloading"

sources = ['MooseX-Role-WithOverloading-0.13.tar.gz']
version = '${DISTNAME}'
name = 'p5'
source_urls = ['http://ftp.nluug.nl/languages/perl/CPAN/MooseX/']

homepage = 'http://search.cpan.org/dist/MooseX-Role-WithOverloading/'

toolchain = {'name': 'goolf', 'version': '1.4.10'}

sanity_check_paths = {
    'files': [],   ## fixme
    'dirs': ['.'], ## fixme
}

parallel = 1         # this is a very conservative choice ## fixme
moduleclass = 'base' # please change this from base to right category ## fixme


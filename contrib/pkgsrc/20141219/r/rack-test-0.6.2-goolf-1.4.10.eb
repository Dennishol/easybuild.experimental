name = 'rack'
version = 'test-0.6.2'
versionsuffix = '-20141219'

description = "rack-test-0.6.2: Simple testing API built on Rack"

sources = [SOURCE_TAR_GZ]

homepage = 'http://github.com/brynary/rack-test'


version = 'stringy-2.110'
versionsuffix = '-20130506'

description = "IO-stringy-2.110: Perl class for I/O on in-core objects like strings and arrays"

sources = ['IO-stringy-2.110.tar.gz']
version = '${DISTNAME}'
name = 'p5'
source_urls = ['http://ftp.nluug.nl/languages/perl/CPAN/IO/']

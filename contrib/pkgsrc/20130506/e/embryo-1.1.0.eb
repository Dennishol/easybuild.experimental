name = 'embryo'
version = '1.1.0'
versionsuffix = '-20130506'

description = "embryo-1.1.0: Embeddable Scripting Language"

sources = ['embryo-1.1.0.tar.bz2']
source_urls = ['http://download.enlightenment.org/releases/']
